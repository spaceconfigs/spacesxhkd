Super_L ; space ; apostrophe
  alacritty

Super_L ; colon
  dmenu_run -l 10 

Super_L ; space ; a ; w
  google-chrome-stable

Super_L ; space ; f ; e ; R
  killall -s SIGUSR1 sxhkd

Super_L ; space ; q ; q
  shutdown -h now

Super_L ; space ; w ; d
  i3-msg "kill"

super + control {h , l}
  i3-msg "workspace {previous, next}"

super {control + shift} + {h, l}
  i3-msg {move container to workspace} {prev_on_output, next_on_output} 

super {_,+ control + shift} + {0-9}
  i3-msg {workspace, move container to workspace} {0-9} 

super + control + shift + {l,h}
  i3-msg "move workspace to output {left,right}"

super + {_,+ shift} + {l, h, j, k}
  i3-msg {focus, move} {right, left, up, down}

Print {_,+ shift}
  scrot {_,-s}

Print + {_,shift +} control
  scrot {-e, -s -e} 'xclip -selection clipboard -t image/png -i $f; rm $f'
